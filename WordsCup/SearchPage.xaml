<Window x:Class="WordsCup.SearchPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WordsCup"
        mc:Ignorable="d"
        Title="SearchPage" Height="450" Width="800" Initialized="Window_Initialized">
    <Grid>
        <Button Content="Reloud" HorizontalAlignment="Right" Margin="0,25,10,0" VerticalAlignment="Top" Click="Button_Click" Height="38" Width="100"/>
        <WebBrowser x:Name="TB" Margin="0,0,150,0"/>
        <Button Content="More" HorizontalAlignment="Right" Margin="0,80,10,0" VerticalAlignment="Top" Click="MoreTextButton" Height="38" Width="100"/>

    </Grid>
</Window>
